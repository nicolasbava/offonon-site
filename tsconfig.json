{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "Node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/Courses/index.jsx",
    "src/components/CoursesItem/index.ksx",
    "src/hooks/use-firebase-data.js",
    "src/components/Eventos/index.jsx",
    "src/components/ContactForm/index.jsx",
    "src/pages/inscribete/index.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
